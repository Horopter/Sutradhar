{
  "name": "@sutradhar/worker",
  "version": "1.0.0",
  "description": "AgentMail microservice worker",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc -p .",
    "typecheck": "tsc --noEmit",
    "start": "node dist/src/server.js",
    "start:prod": "NODE_ENV=production node dist/src/server.js",
    "smoke:prod": "bash scripts/smoke-prod.sh",
    "health": "bash scripts/health-check.sh",
    "send:self": "bash curl/agentmail-send-self.sh",
    "replay:webhook": "bash curl/replay-webhook.sh",
    "send:test": "bash curl/agentmail-send.sh",
    "convex:create": "bash curl/convex-create-session.sh",
    "convex:append": "bash curl/convex-append-message.sh",
    "convex:list": "bash curl/convex-list-messages.sh",
    "diag:convex": "curl -sS http://localhost:4001/convex/diag | jq .",
    "diag:agentmail": "bash curl/diag-agentmail.sh",
    "diag:hyperspell": "bash curl/diag-hyperspell.sh",
    "act:slack": "bash curl/composio-slack.sh",
    "act:cal": "bash curl/composio-calendar.sh",
    "act:gh": "bash curl/composio-github.sh",
    "diag:composio": "bash curl/diag-composio.sh",
    "actions:list": "bash curl/actions-list.sh",
    "answer:test": "bash curl/answer.sh",
    "retrieval:local:index": "bash curl/retrieval-local-index.sh",
    "retrieval:local:query": "bash curl/retrieval-local-query.sh",
    "retrieval:answer": "bash curl/retrieval-answer.sh",
    "retrieval:hs:seed": "bash curl/retrieval-hs-seed.sh",
    "forum:post": "bash curl/forum-post.sh",
    "demo:happy": "bash scripts/demo-happy-paths.sh",
    "test:all": "npm run test:unified && npm run test:concurrent && npm run test:extensive && npm run test:robustness",
    "test:api": "bash scripts/test-all-apis.sh",
    "test:edge": "bash scripts/test-edge-cases.sh",
    "test": "bash scripts/test-all-apis.sh && bash scripts/test-edge-cases.sh",
    "admin:toggle": "bash curl/toggle.sh",
    "llm:answer": "bash curl/llm-answer.sh",
    "llm:summarize": "bash curl/llm-summarize.sh",
    "llm:escalate": "bash curl/llm-escalate.sh",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:jest": "jest",
    "test:unified-action": "bash scripts/test-unified-action-service.sh",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "health:check": "bash scripts/health-check.sh",
    "demo": "bash scripts/demo-happy-paths.sh",
    "test:unified": "bash scripts/test-unified-api-comprehensive.sh",
    "test:concurrent": "bash scripts/test-concurrent-agents.sh",
    "test:extensive": "bash scripts/test-unified-api-extensive.sh",
    "test:robustness": "bash scripts/test-robustness.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@livekit/agents": "^1.0.15",
    "@livekit/agents-plugin-openai": "^1.0.15",
    "@livekit/rtc-node": "^0.13.20",
    "@sentry/node": "^10.22.0",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/marked": "^5.0.2",
    "body-parser": "^1.20.2",
    "composio-core": "^0.5.39",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "hyperspell": "^0.26.0",
    "jsonwebtoken": "^9.0.2",
    "livekit-client": "^2.15.14",
    "livekit-server-sdk": "^2.14.0",
    "marked": "^16.4.1",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.4",
    "node-fetch": "^3.3.2",
    "playwright": "^1.56.1",
    "prom-client": "^15.1.3",
    "redis": "^4.6.13",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
